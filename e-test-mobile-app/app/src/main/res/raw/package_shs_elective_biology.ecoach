        {
          "id": "212686",
          "text": "<p>freshwater streams</p>",
          "value": "0",
          "solution": ""
        },
        {
          "id": "212687",
          "text": "<p>human habitations</p>",
          "value": "0",
          "solution": ""
        },
        {
          "id": "212688",
